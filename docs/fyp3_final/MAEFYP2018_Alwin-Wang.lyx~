\end_layout

\begin_layout Subsubsection
Add Vorticity Fields
\end_layout

\begin_layout Standard
Four additional fields were required in order to determine the values of
 
\begin_inset Formula $\omega,\frac{\partial\omega}{\partial x},\frac{\partial\omega}{\partial y},\frac{\partial p}{\partial x},\frac{\partial p}{\partial y}$
\end_inset

.
 This was achieved by adding five additional 
\begin_inset listings
inline true
status open

\begin_layout Plain Layout

AuxField
\end_layout

\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset listings
lstparams "language={C++},numbers=left,showstringspaces=false,tabsize=2,commentstyle={\itshape\ttfamily\color{commentgreen}},keywordstyle={\bfseries\color{weborange}},stringstyle={\color{frenchplum}}"
inline false
status open

\begin_layout Plain Layout

// Add new flag case for command line
\end_layout

\begin_layout Plain Layout

case 'G': flag[VORTGEN]      = true; break;
\end_layout

\begin_layout Plain Layout

// Declare VortGen
\end_layout

\begin_layout Plain Layout

  vector<AuxField*>          VortGen;
\end_layout

\begin_layout Plain Layout

  vector<real_t*>            VortGenData;
\end_layout

\begin_layout Plain Layout

// Handle flags from command line
\end_layout

\begin_layout Plain Layout

 if (need[VORTGEN]) {
\end_layout

\begin_layout Plain Layout

    VortGen    .resize (5);
\end_layout

\begin_layout Plain Layout

    VortGenData.resize (5);
\end_layout

\begin_layout Plain Layout

    for (i = 0; i < 5; i++) { 
\end_layout

\begin_layout Plain Layout

      VortGenData[i]   = new real_t [allocSize];
\end_layout

\begin_layout Plain Layout

      VortGen[i]       = new AuxField (VortGenData[i],nz,elmt,'k'+i);
\end_layout

\begin_layout Plain Layout

      addField[iAdd++] = VortGen[i]; 
\end_layout

\begin_layout Plain Layout

    }
\end_layout

\begin_layout Plain Layout

  }
\end_layout

\begin_layout Plain Layout

// while (getDump(D, file))
\end_layout

\begin_layout Plain Layout

//  for(i = 0; i < allocSize; i++)
\end_layout

\begin_layout Plain Layout

      if (need[VORTGEN]) { // Only for 2 dimensions 
\end_layout

\begin_layout Plain Layout

        tensor3::vorticity (tensor, vort);
\end_layout

\begin_layout Plain Layout

        VortGenData[4][i] = vort[2];          // o = omega
\end_layout

\begin_layout Plain Layout

      }
\end_layout

\begin_layout Plain Layout

//  finish looping over each point i
\end_layout

\begin_layout Plain Layout

    if (need[VORTGEN]) { // Only for 2 dimensions 
\end_layout

\begin_layout Plain Layout

      (*VortGen[0]  = *VortGen[4]).gradient(0); // k = do/dx
\end_layout

\begin_layout Plain Layout

      (*VortGen[1]  = *VortGen[4]).gradient(1); // l = do/dy
\end_layout

\begin_layout Plain Layout

      (*VortGen[2]  = *pressure)  .gradient(0); // m = dp/dx
\end_layout

\begin_layout Plain Layout

      (*VortGen[3]  = *pressure)  .gradient(1); // n = dp/dy
\end_layout

\begin_layout Plain Layout

    }
\end_layout

\begin_layout Plain Layout

// finish getDump
\end_layout

\begin_layout Plain Layout

\begin_inset Caption Standard

\begin_layout Plain Layout
Exerts of modifications to addfield.cpp
\end_layout

\end_inset


\end_layout

\end_inset


\end_layout

