\begin_layout Standard
\begin_inset listings
lstparams "language={C++},numbers=left,showstringspaces=false,tabsize=2,commentstyle={\itshape\ttfamily\color{commentgreen}},keywordstyle={\bfseries\color{weborange}},stringstyle={\color{frenchplum}}"
inline false
status open

\begin_layout Plain Layout

// static void printWalls (...)
\end_layout

\begin_layout Plain Layout

  std::cout.precision(16);
\end_layout

\begin_layout Plain Layout

  cout << 
\backslash

\end_layout

\begin_layout Plain Layout

    setw(24) << "x"  << setw(24) << "y"  << 
\backslash

\end_layout

\begin_layout Plain Layout

    setw(24) << "nx" << setw(24)     << "ny" << setw(24) << "area" << 
\backslash

\end_layout

\begin_layout Plain Layout

    setw(10) << "k"  << setw(10) << "s"  << endl;
\end_layout

\begin_layout Plain Layout

  for (i = 0; i < Nedge; i++)
\end_layout

\begin_layout Plain Layout

    if (strstr (bman -> groupInfo (edge[i] -> group), "wall")) {
\end_layout

\begin_layout Plain Layout

      k = edge[i] -> elmt;
\end_layout

\begin_layout Plain Layout

      s = edge[i] -> side;
\end_layout

\begin_layout Plain Layout

      elmt[k] -> sideGeom(s, &xs[0], &ys[0], &nxs[0], &nys[0], &area[0]);
\end_layout

\begin_layout Plain Layout

      for (j = 0; j < np; j++) {
\end_layout

\begin_layout Plain Layout

	      cout << 
\backslash

\end_layout

\begin_layout Plain Layout

	        setw(24) << xs [j] << setw(24) << ys [j] << 
\backslash

\end_layout

\begin_layout Plain Layout

	        setw(24) << nxs[j] << setw(24) << nys[j] << 
\backslash

\end_layout

\begin_layout Plain Layout

	        setw(24) << area[j] << 
\backslash

\end_layout

\begin_layout Plain Layout

	        setw(10) << k + 1  << setw(10) << s + 1  << endl;
\end_layout

\begin_layout Plain Layout

      }
\end_layout

\begin_layout Plain Layout

    }
\end_layout

