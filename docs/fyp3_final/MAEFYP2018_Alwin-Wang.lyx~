\end_layout

\end_inset


\begin_inset Formula 
\begin{align}
\Phi_{p}=\Phi_{0}+\Phi_{1} & =-\frac{\gamma_{0}}{2\pi}\left[\left(x-x_{1}\right)\theta_{1}-\left(x-x_{2}\right)\theta_{2}+\frac{z}{2}\ln\left(\frac{r_{1}^{2}}{r_{2}^{2}}\right)\right]\nonumber \\
 & \qquad-\frac{\gamma_{1}}{2\pi}\left[\frac{xz}{2}\ln\left(\frac{r_{1}^{2}}{r_{2}^{2}}\right)+\frac{z}{2}\left(x_{1}-x_{2}\right)+\frac{x^{2}-x_{1}^{2}-z^{2}}{2}\theta_{1}-\frac{x^{2}-x_{2}^{2}-z^{2}}{2}\theta_{2}\right]\\
u_{p}=u_{0}+u_{1} & =\frac{\gamma_{0}}{2\pi}\left(\theta_{2}-\theta_{1}\right)-\frac{\gamma_{1}}{2\pi}\left[z\ln\left(\frac{r_{1}}{r_{2}}\right)-x\left(\theta_{2}-\theta_{1}\right)\right]\\
w_{p}=w_{0}+w_{1} & =-\frac{\gamma_{0}}{2\pi}\ln\left(\frac{r_{1}}{r_{2}}\right)-\frac{\gamma_{1}}{2\pi}\left[x\ln\left(\frac{r_{1}}{r_{2}}\right)+\left(x_{1}-x_{2}\right)+z\left(\theta_{2}-\theta_{1}\right)\right]
\end{align}

