\begin_layout Plain Layout
Vorticity Generation
\end_layout

\begin_layout Itemize
Morton 1984: Generation of vorticity for a flat plate moving in its own
 plane is from two mechanisms
\begin_inset Formula 
\[
-\nu\left(\frac{\partial\vec{\omega}}{\partial z}\right)_{0}=-\frac{1}{\rho}\left[\left(\vec{n}\times\nabla\right)\vec{p}\right]_{0}-\vec{n}\times\frac{\text{d}V}{\text{d}t}
\]

\end_inset


\end_layout

\begin_layout Itemize
Blackburn 1999: Mechanisms oppose each other for vorticity generation
\begin_inset Formula 
\[
-\nu\vec{n}\cdot\nabla\vec{\omega}=-\vec{n}\times\left(\nabla\vec{P}+\vec{a}\right)
\]

\end_inset


\begin_inset Formula 
\[
\ointop-\nu\vec{n}\cdot\nabla\vec{\omega}\,\text{d}S=\ointop-\nu\frac{\partial\omega}{\partial n}\,\text{d}S=0
\]

\end_inset


\end_layout

